<svg width="640" height="640" xmlns="http://www.w3.org/2000/svg">
  <!-- Preview Image for Klondike Solitaire -->
  <rect width="640" height="640" fill="#1a1a1a"/>

  <!-- Title -->
  <text x="320" y="80" font-family="Arial, sans-serif" font-size="48" font-weight="300" fill="#f0f0f0" text-anchor="middle">
    Klondike Solitaire
  </text>

  <!-- Card examples -->
  <!-- Card 1 - Ace of Hearts -->
  <rect x="80" y="150" width="100" height="140" rx="8" fill="#fff" stroke="#ccc" stroke-width="2"/>
  <text x="90" y="175" font-family="Arial" font-size="18" fill="#dc2626" font-weight="bold">A</text>
  <text x="90" y="195" font-family="Arial" font-size="24" fill="#dc2626">♥</text>
  <text x="130" y="220" font-family="Arial" font-size="48" fill="#dc2626">♥</text>

  <!-- Card 2 - King of Spades -->
  <rect x="200" y="150" width="100" height="140" rx="8" fill="#fff" stroke="#ccc" stroke-width="2"/>
  <text x="210" y="175" font-family="Arial" font-size="18" fill="#000" font-weight="bold">K</text>
  <text x="210" y="195" font-family="Arial" font-size="24" fill="#000">♠</text>
  <text x="250" y="220" font-family="Arial" font-size="48" fill="#000">♠</text>

  <!-- Card Back - Default -->
  <rect x="320" y="150" width="100" height="140" rx="8" fill="#000" stroke="#666" stroke-width="2"/>

  <!-- Card 3 - Queen of Diamonds -->
  <rect x="440" y="150" width="100" height="140" rx="8" fill="#fff" stroke="#ccc" stroke-width="2"/>
  <text x="450" y="175" font-family="Arial" font-size="18" fill="#dc2626" font-weight="bold">Q</text>
  <text x="450" y="195" font-family="Arial" font-size="24" fill="#dc2626">♦</text>
  <text x="490" y="220" font-family="Arial" font-size="48" fill="#dc2626">♦</text>

  <!-- Pattern examples -->
  <text x="320" y="340" font-family="Arial" font-size="24" fill="#f0f0f0" text-anchor="middle">
    Customizable Card Backs
  </text>

  <!-- Pattern card 1 - Plaid -->
  <defs>
    <pattern id="previewPlaid" x="0" y="0" width="20" height="20" patternUnits="userSpaceOnUse">
      <rect width="20" height="20" fill="#8B4513"/>
      <rect x="0" y="9" width="20" height="2" fill="#D2691E" opacity="0.7"/>
      <rect x="9" y="0" width="2" height="20" fill="#D2691E" opacity="0.7"/>
    </pattern>
  </defs>
  <rect x="140" y="370" width="100" height="140" rx="8" fill="url(#previewPlaid)" stroke="#666" stroke-width="2"/>
  <text x="190" y="525" font-family="Arial" font-size="14" fill="#f0f0f0" text-anchor="middle">Plaid</text>

  <!-- Pattern card 2 - Cowboy -->
  <defs>
    <pattern id="previewCowboy" x="0" y="0" width="30" height="30" patternUnits="userSpaceOnUse">
      <rect width="30" height="30" fill="#654321"/>
      <circle cx="8" cy="8" r="4" fill="#8B7355" opacity="0.6"/>
      <circle cx="22" cy="22" r="4" fill="#8B7355" opacity="0.6"/>
    </pattern>
  </defs>
  <rect x="270" y="370" width="100" height="140" rx="8" fill="url(#previewCowboy)" stroke="#666" stroke-width="2"/>
  <text x="320" y="525" font-family="Arial" font-size="14" fill="#f0f0f0" text-anchor="middle">Cowboy</text>

  <!-- Pattern card 3 - Op Art -->
  <defs>
    <pattern id="previewOpArt" x="0" y="0" width="30" height="30" patternUnits="userSpaceOnUse">
      <rect width="30" height="30" fill="#000"/>
      <circle cx="15" cy="15" r="13" fill="none" stroke="#fff" stroke-width="1.5"/>
      <circle cx="15" cy="15" r="8" fill="none" stroke="#fff" stroke-width="1.5"/>
    </pattern>
  </defs>
  <rect x="400" y="370" width="100" height="140" rx="8" fill="url(#previewOpArt)" stroke="#666" stroke-width="2"/>
  <text x="450" y="525" font-family="Arial" font-size="14" fill="#f0f0f0" text-anchor="middle">Op Art</text>

  <!-- Footer text -->
  <text x="320" y="580" font-family="Arial" font-size="18" fill="#5399B5" text-anchor="middle">
    Drag &amp; Drop • Pattern Packs • Mobile Friendly
  </text>
</svg>
